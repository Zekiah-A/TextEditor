<Application xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="using:TextEditorAvalonia"
			 x:Class="TextEditorAvalonia.App"
			 RequestedThemeVariant="Default">
	<Application.DataTemplates>
		<local:ViewLocator/>
	</Application.DataTemplates>
	
	<Application.Styles>
		<FluentTheme/>
	</Application.Styles>
	
	<Application.Resources>
		<FontFamily x:Key="SegMDL2">avares://TextEditorAvalonia/Assets/Fonts#Segoe MDL2 Assets</FontFamily>
	</Application.Resources>
</Application>
